/******** You are given a number N. You have to find the number of operations required to reach N from 0.
You have 2 operations available:
1- Double the number
2- Add one to the number **********/

int main() 
{
	cin>>t;
	for(int i=0;i<t;i++)
	{
	    cin>>n;
	    int cnt=0;
	    while(n>0)
	    {
	        if(n%2!=0)
	        n--;
	        else n=n/2;
	        cnt++;
	    }
	    cout<<cnt<<endl;
	}
	
	return 0;
}
