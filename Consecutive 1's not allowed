/* Given a positive integer N, count all possible distinct binary strings of length N such that there are no consecutive 1â€™s. 
Output your answer mod 10^9 + 7. */

#include <bits/stdc++.h>
using namespace std;
long long int t,n,x;
long long int mod=1e9 + 7;
int a[1000],b[1000];
int main() 
{
	cin>>t;
	a[1]=1,b[1]=1;
	for(int j=2;j<=100;j++)
	    {
	     a[j]=(a[j-1]+b[j-1])%mod;
	     b[j]=a[j-1];
	    }
	for(int i=0;i<t;i++)
	{
	    cin>>n;
	    cout<<(a[n]+b[n])%mod<<endl;
	}
	return 0;
}
